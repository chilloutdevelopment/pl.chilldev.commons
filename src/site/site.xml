        <logo
            name="Wrzasq.pl"
            href="http://wrzasq.pl"
            img="http://static.chilldev.pl/images/wrzasqpl.png"
            alt="Wrzasq.pl"
            title="Wrzasq.pl"/>
    </poweredBy>

    <body>
        <menu ref="modules"/>

        <menu name="Contribution guide">
            <item name="Git workflow" href="gitflow.html"/>
        </menu>

        <menu ref="reports"/>
    </body>

    <custom>
        <fluidoSkin>
            <gitHub>
                <projectId>chilloutdevelopment/pl.chilldev.commons</projectId>
                <ribbonOrientation>right</ribbonOrientation>
